{
  "name": "oas-stack-playground",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:config": "tsc -p tsconfig.node.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build src/client --outDir ../../dist/client",
    "build": "pnpm build:config && pnpm build:server && pnpm build:client",
    "play": "node --loader ts-node/esm src/server/server.ts",
    "serve": "node dist/server/server.js"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-react-jsx-development": "^7.18.6",
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/system": "^2.5.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fastify/cors": "^8.2.0",
    "@fastify/vite": "^3.0.8",
    "@preact/preset-vite": "^2.5.0",
    "babel-plugin-transform-hook-names": "^1.0.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "framer-motion": "^9.0.4",
    "oas30-to-zod": "workspace:*",
    "preact": "^10.12.1",
    "react": "npm:@preact/compat@^17.1.2",
    "react-ace": "^10.1.0",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "react-icons": "^4.7.1",
    "strip-ansi": "^7.0.1",
    "vite": "^4.1.1",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "ace-builds": "^1.15.2",
    "ace-code": "^1.15.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
